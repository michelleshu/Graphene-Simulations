function root = findroot(F)
    min = 0;
    max = numel(F);
    while true
        % Base cases
        if (min == max)
            root = min;
            return;
            
        elseif (max - min == 1)
            root = (max + min) / 2;
            return;
            
            
        mid = (min + max) / 2;
        if ((F(min) * F(mid)) < 0)
            min = 
    end
    
end